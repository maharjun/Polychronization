    <IncludePath>$(INTEL_TBB_ROOT)\include;$(IncludePath)</IncludePath>
    <LibraryPath>$(INTEL_TBB_ROOT)\build\vs2010\intel64\Release;$(LibraryPath)</LibraryPath>
